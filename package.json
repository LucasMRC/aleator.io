{
	"name": "aleator.io",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json"
	},
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tsconfig/svelte": "^5.0.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tslib": "^2.6.2",
		"typescript": "^5.5.0",
		"vite": "^5.4.4"
	},
	"dependencies": {
		"eslint": "^8.56.0",
		"standard": "^17.1.0"
	},
	"eslintConfig": {
		"extends": [
			"standard"
		],
		"rules": {
			"indent": [
				"error",
				4
			],
			"semi": [
				"error",
				"always"
			]
		},
		"ignorePatterns": [
			"**/*.html"
		]
	}
